{
    "tasks": {
        "refresh": "deno run --allow-read --allow-env --allow-net  scripts/registerCommands.js",
        "start": "deno run --allow-env --allow-read --allow-net index.js",
        "guild-refresh": "deno run --allow-read --allow-env --allow-net  scripts/registerGuildCommands.js",
        "dev": "deno task guild-refresh && deno task start",
        "create-service": "deno run --allow-read --allow-run --allow-env --allow-write scripts/createService.js",
        "remove-service": "deno run --allow-read --allow-run --allow-env --allow-write scripts/removeService.js"
    }
}